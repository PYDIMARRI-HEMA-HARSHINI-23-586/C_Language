#include <stdio.h>
int main()
 {
    int n, i,min,a[100];

    printf("Enter the numbers of elements: ");
    scanf("%d", &n);
    for(i=0;i<=n;i++)
    {
    	scanf("%d",&a[i]);
    	min = a[0];
	}
	for(i=0;i<=n;i++)
	{
    	if(min > a[i])
    	{
    		min = a[i];
		}
	}
	 printf("min = %d\n",min);
}
